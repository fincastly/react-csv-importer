{
  "name": "react-csv-importer",
  "version": "0.2.3",
  "description": "React CSV import widget with user-customizable mapping",
  "keywords": [
    "react",
    "csv",
    "upload",
    "parser",
    "import",
    "preview",
    "papaparse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beamworks/react-csv-importer"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "eslint --max-warnings=0 --ext ts --ext tsx src",
    "lint-fix": "eslint --max-warnings=0 --ext ts --ext tsx src --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dist": "webpack --mode production && dts-bundle-generator -o dist/index.d.ts src/index.ts"
  },
  "author": "Nick Matantsev <nick.matantsev@beamworks.io>",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --max-warnings=0",
    "src/**/*.scss": "stylelint"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.21",
    "@types/papaparse": "^5.2.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "dotenv-webpack": "^2.0.0",
    "dts-bundle-generator": "^5.4.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.1.0",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^0.11.1",
    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "blob-polyfill": "^4.0.20200601",
    "papaparse": "^5.3.0",
    "react-dropzone": "^11.0.3",
    "react-use-gesture": "^7.0.16",
    "readable-web-to-node-stream": "^2.0.0"
  }
}
